class RLEIterator {
    int size=0;
    int[] arr;
    int idx;
    public RLEIterator(int[] encoding) {
        this.arr = encoding;
        this.idx=0;
    }
    
    public int next(int n) {
        while(idx < arr.length){
            if(arr[idx] >= n){
                arr[idx] -= n;
                return arr[idx+1];
            }else{
                n -= arr[idx];
                idx += 2;
            }
        }
        return -1;
    }
}

/**
 * Your RLEIterator object will be instantiated and called as such:
 * RLEIterator obj = new RLEIterator(encoding);
 * int param_1 = obj.next(n);
 */
