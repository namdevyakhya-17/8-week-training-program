package Recursion;

public class PrintSum {
    public static int getSum(int n){
        if(n < 10){
            return n;
        }

        int rem = n%10;
        return getSum(n/10) + rem;
    }
    public static void main(String[] args) {
        int n = 12345;
        int sum = getSum(n);
        System.out.println("Sum if digits of number 12345: " + sum);
    }
}
