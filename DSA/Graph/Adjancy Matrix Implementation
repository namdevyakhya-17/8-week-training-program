				      
class Solution{
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int e = sc.nextInt();
		int[][] mat = new int[n+1][n+1];
		for(int i=0; i<e; i++){
			int src = sc.nextInt();
			int dest = sc.nextInt();
			mat[src][dest] = 1;
			mat[dest][src] = 1;
		}
		
		for(int i=0; i<=n; i++){
			for(int j=0; j<=n; j++){
				System.out.print(mat[i] + " ");
			}
			System.out.println();
		}
	}
}
		
		
		
