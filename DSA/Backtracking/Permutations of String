class Solution {
    public static void helperOptimal(StringBuilder str, int i, ArrayList<String> ans, HashSet<String> set){
        if(i==str.length()){
            if(!set.contains(str.toString())){
                set.add(str.toString());
                ans.add(str.toString());
            }
            return;
        }

        for(int j=i; j<str.length(); j++){
            swap(str, i, j);
            helperOptimal(str, i+1, ans,set);
            swap(str, i, j);
        }
    }
    
    public static void swap(StringBuilder str, int i, int j){
        char ch1 = str.charAt(i);
        char ch2 = str.charAt(j);
        str.setCharAt(i, ch2);
        str.setCharAt(j, ch1);
    }
    public ArrayList<String> findPermutation(String s) {
        // Code here
        ArrayList<String> res = new ArrayList<>();
        helperOptimal(new StringBuilder(s),0,res,new HashSet<String>());
        return res;
    }
}
